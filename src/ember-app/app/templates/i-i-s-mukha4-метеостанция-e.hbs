<h3 class="ui header">{{t "forms.i-i-s-mukha4-метеостанция-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.название
      class=(if (v-get validationModel "название" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.название-caption")
      data-test-i-i-s-mukha4-метеостанция-e-название=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "название" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.iDИдентификатор
      class=(if (v-get validationModel "iDИдентификатор" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.iDИдентификатор-caption")
      data-test-i-i-s-mukha4-метеостанция-e-iDИдентификатор=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "iDИдентификатор" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.местоположение
      class=(if (v-get validationModel "местоположение" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.местоположение-caption")
      data-test-i-i-s-mukha4-метеостанция-e-местоположение=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "местоположение" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.широта
      class=(if (v-get validationModel "широта" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.широта-caption")
      data-test-i-i-s-mukha4-метеостанция-e-широта=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "широта" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.долгота
      class=(if (v-get validationModel "долгота" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.долгота-caption")
      data-test-i-i-s-mukha4-метеостанция-e-долгота=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "долгота" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.городID
      class=(if (v-get validationModel "городID" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.городID-caption")
      data-test-i-i-s-mukha4-метеостанция-e-городID=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "городID" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.влажность
      class=(if (v-get validationModel "влажность" "isInvalid") "error")
      label=(t "forms.i-i-s-mukha4-метеостанция-e.влажность-caption")
      data-test-i-i-s-mukha4-метеостанция-e-влажность=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "влажность" "message")}}
  </div>
</form>
